{# templates/your_template.html.twig #}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Object Creation Result</title>
</head>
<body>
{% if success %}
    <div class="alert alert-success">
        {{ entity }} created successfully.
    </div>
{% else %}
    <div class="alert alert-danger">
        {{ entity }} was not created.
    </div>
{% endif %}
</body>
</html>
